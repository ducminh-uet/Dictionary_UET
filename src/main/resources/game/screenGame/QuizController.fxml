<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="game.screenGame.QuizController">
   <children>
      <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#backToMenu" text="Back" />
      <Label fx:id="myLabel" layoutX="14.0" layoutY="50.0" text="Score: 0" />
      <Label fx:id="QuestionText" layoutX="14.0" layoutY="80.0" text="Question Text" />
      <CheckBox fx:id="myCheckBox1" layoutX="91.0" layoutY="150.0" mnemonicParsing="false" onAction="#chooseYourAnswer" text="Option 1" />
      <CheckBox fx:id="myCheckBox2" layoutX="91.0" layoutY="192.0" mnemonicParsing="false" text="Option 2" />
      <CheckBox fx:id="myCheckBox3" layoutX="91.0" layoutY="234.0" mnemonicParsing="false" text="Option 3" />
      <CheckBox fx:id="myCheckBox4" layoutX="91.0" layoutY="276.0" mnemonicParsing="false" text="Option 4" />
   </children>
</AnchorPane>
